<h2> New contact</h2>
<%= form_for :contact, url: contacts_path do |f| %>

	<p>
		<%= f.label :name %><br>
		<%= f.text_area :name %>
	</p>
	<p>
		<%= f.label :phone %><br>
		<%= f.text_area :phone %>
	</p>
	<p>
		<%= f.label :address %><br>
		<%= f.text_area :address %>
	</p>
	<p>
		<%= f.submit %>
	</p>
	<% end -%>